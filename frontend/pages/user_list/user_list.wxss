/* pages/user_list/user_list.wxss */
.project {
  width: 700rpx;
  height: 180rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  background-color: #fff;
  border-radius: 20rpx;
  /* box-shadow: 0px 2px 5px #dfdede; */
  position: relative;
  border-bottom: 2rpx dashed #efeff1;
  
}

.headimage {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin-left: 0rpx;
  margin-top: 15rpx;
  /* border:1rpx solid #ffd92e; */
}

.projecttext {
  position: absolute;
  top: 70rpx;
  left: 180rpx;
}

.nameicon {
  width: 50rpx;
  height: 30rpx;
  position:absolute;
  top:100rpx;
  right:120rpx;

}

.icon{
  margin-top: 80rpx;
  float: right;
}

.project1{
  width:600rpx;
  float:left;
}

.tabbar{
  width:100%;
  height:100rpx;
  position:fixed;
  bottom:0rpx;
  display: flex;
}

.tabImage{
  display:inline-block;
  width:25%;
  height:100rpx;
  margin-right:10rpx;
}

.textBar{
  width:45%;
  height:100rpx;
  text-align:center;
  font-size:50rpx;  
  align-items:center;
  justify-content: center;
  display:flex;
  background-color:pink;
}